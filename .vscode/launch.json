    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch logsrouter",
            "program": "${workspaceFolder}/routes/logRouter.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch odata Server",
            "program": "${workspaceFolder}/odata/dataserver.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch requestSimulation",
            "program": "${workspaceFolder}/utils/requestSimulator.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch logRetrieval",
            "program": "${workspaceFolder}/utils/logRetrieval.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch  databaseConfig",
            "program": "${workspaceFolder}/db/database.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch LogParser",
            "program": "${workspaceFolder}/utils/logparser.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
               {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\www"
        }
    ]
}
{
	"xsappname": "data-monitoring-list",
	"tenant-mode": "dedicated",
	"scopes": [
		{
			"name": "$XSAPPNAME.read",
			"description": "With this scope, USER can read monitoring data."
        },
        { 
            "name" : "$XSAPPNAME.Edit", 
            "description" : "USER can edit  monitoring data" }, 
           { 
            "name" : "$XSAPPNAME.Delete", 
            "description" : "user can delete  monitoring data"  } 
	],

	"role-templates": [
		{
			"name": "Viewer",
			"description": "Role to get the list of logs or whitelists",
			"scope-references": [
				"$XSAPPNAME.Read"
			]
        },
        {
			"name": "Editor",
			"description": "Role to change the list of logs or whitelists",
			"scope-references": [ 
                "$XSAPPNAME.Edit", 
                "$XSAPPNAME.Delete",
                "$XSAPPNAME.Read" ]
		}
	],
	"role-collections": [
		{
			"name": "DataMonitoringListViewer",
			"description": "Data monitoring List Viewer",
			"role-template-references": [
				"$XSAPPNAME.Viewer"
			]
        },
        {
			"name": "DataMonitoringListEditor",
			"description": "Data monitoring List Editor",
			"role-template-references": [
				"$XSAPPNAME.Editor"
			]
        }
        
	]
}